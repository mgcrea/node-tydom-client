{
  "name": "tydom-client",
  "version": "0.4.0",
  "description": "Easily manage Tydom hardware by Delta Dore from Node.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Olivier Louvignes <olivier@mgcrea.io>",
  "repository": "github:mgcrea/node-tydom-client",
  "license": "MIT",
  "scripts": {
    "start": "yarn spec:watch",
    "test": "yarn pretty && yarn lint && yarn spec",
    "spec": "NODE_TLS_REJECT_UNAUTHORIZED=0 CI=true jest --runInBand --colors",
    "spec:watch": "yarn spec --watch",
    "spec:coverage": "yarn spec --coverage",
    "lint": "eslint --ext .ts src/",
    "pretty": "prettier --check '{src,test}/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "build": "yarn build:source && yarn build:typings",
    "build:source": "babel --source-maps --extensions .js,.ts --out-dir lib/ --delete-dir-on-start --ignore **/__tests__ src/",
    "build:typings": "tsc --declaration",
    "build:watch": "yarn build:source --watch --verbose",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "http-parser-js": "^0.5.2",
    "node-fetch": "^2.6.0",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "7.7.2",
    "@babel/node": "7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@babel/runtime": "7.7.2",
    "@types/debug": "^4.1.5",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.7",
    "@types/node-fetch": "^2.5.3",
    "@types/ws": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "nock": "^11.7.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.2"
  }
}
